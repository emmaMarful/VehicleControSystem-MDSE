                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="VcuSystem::PinGroup" referenceName="pingroup" variableName="groupinstance">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:groupinstance">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="VcuSystem::Pin" referenceName="pin" variableName="pininstance">
